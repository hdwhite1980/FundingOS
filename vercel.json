    {
      "path": "/api/sync/grants-gov",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/data/sync-opportunities",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/ai/agent/cron/start-agents",
      "schedule": "0 */1 * * *"
    },
    {
      "path": "/api/cron/sam-gov-sync",
      "schedule": "0 12,14,16,17,19,20,22,23,1 * * *"
    }
  ],
  "functions": {
    "app/api/sync/grants-gov/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/sam-gov-sync/route.ts": {
      "maxDuration": 30
    },
    "pages/api/data/sync-opportunities.js": {
      "maxDuration": 300
    },
    "pages/api/ai/analyze-opportunity.js": {
      "maxDuration": 60
    },
    "pages/api/ai/generate-application.js": {
      "maxDuration": 120
    },
    "pages/api/ai/agent/cron/start-agents.js": {
      "maxDuration": 60
    }
